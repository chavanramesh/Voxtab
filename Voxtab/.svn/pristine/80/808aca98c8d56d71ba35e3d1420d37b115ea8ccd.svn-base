<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="58dp">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="354dp"
        android:layout_height="64dp"
        android:layout_gravity="center"
        android:src="@drawable/heder_image_order" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:scrollbarThumbVertical="@android:color/transparent">


        <LinearLayout
            android:id="@+id/lin_parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="@dimen/activity_horizontal_margin">

            <LinearLayout
                android:id="@+id/lin_file_info"
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:layout_gravity="center"
                android:background="@drawable/file_info_bg"
                android:gravity="center"
                android:orientation="vertical"
                android:visibility="visible">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="1">


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="0.45"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lbl_total_duration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"
                            android:singleLine="true"
                            android:text="@string/total_duration"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/lbl_total_fee"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"
                            android:singleLine="true"
                            android:text="@string/total_fee"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/lbl_delivery_date_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"

                            android:text="@string/delivery_date_time"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size"
                            android:textStyle="bold" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="4dp"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=":"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="4dp"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=":"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="4dp"
                            android:layout_marginTop="4dp"
                            android:singleLine="true"
                            android:text=":"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_marginTop="2dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:layout_weight="0.55"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txt_total_duration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"
                            android:singleLine="true"
                            android:text="00:00:00"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size" />

                        <TextView
                            android:id="@+id/txt_total_fee"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"
                            android:singleLine="true"
                            android:text="$20"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_delivery_date_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="4dp"
                            android:maxLines="2"
                            android:text="12 Dec 2016 11:30am"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/secondary_text_size" />


                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="right"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txt_change"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="right"
                        android:text="@string/change"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/tertiary_text_size" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lin_service_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/btn_padding"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="58dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/btn_padding">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:drawableBottom="@drawable/header_underline"
                        android:drawablePadding="4dp"
                        android:padding="@dimen/btn_padding"
                        android:singleLine="true"
                        android:text="@string/select_service_details"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/primary_text_size"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp">

                    <Spinner
                        android:id="@+id/spn_service_type"
                        android:layout_width="fill_parent"
                        android:layout_height="36dp"
                        android:layout_margin="4dp"
                        android:background="@drawable/dropdown_box"
                        android:paddingRight="18dp"
                        android:textSize="@dimen/txtmemid_size" />

                    <Spinner
                        android:id="@+id/spn_transcription_type"
                        android:layout_width="fill_parent"
                        android:layout_height="36dp"
                        android:layout_margin="4dp"
                        android:background="@drawable/dropdown_box"
                        android:paddingRight="15dp"
                        android:textSize="@dimen/txtmemid_size" />

                    <Spinner
                        android:id="@+id/spn_timestamp_duration"
                        android:layout_width="fill_parent"
                        android:layout_height="36dp"
                        android:layout_margin="4dp"
                        android:background="@drawable/dropdown_box"
                        android:paddingRight="15dp"
                        android:textSize="@dimen/txtmemid_size" />

                    <Spinner
                        android:id="@+id/spn_value_added_services"
                        android:layout_width="fill_parent"
                        android:layout_height="36dp"
                        android:layout_margin="4dp"
                        android:background="@drawable/dropdown_box"
                        android:paddingRight="15dp"
                        android:textSize="@dimen/txtmemid_size" />


                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lin_free_services"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:orientation="vertical"
                android:padding="@dimen/btn_padding">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/btn_padding">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:drawableBottom="@drawable/header_underline"
                        android:drawablePadding="4dp"
                        android:padding="@dimen/btn_padding"
                        android:text="@string/free_services"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/primary_text_size"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="4dp"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:weightSum="1">

                        <TextView
                            android:id="@+id/ch_free_service1"
                            android:layout_width="0dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size" />

                        <TextView
                            android:id="@+id/ch_free_service2"
                            android:layout_width="0dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:weightSum="1">

                        <TextView
                            android:id="@+id/ch_free_service3"
                            android:layout_width="0dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size" />

                        <TextView
                            android:id="@+id/ch_free_service4"
                            android:layout_width="0dp"
                            android:layout_height="32dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="4dp"
                        android:paddingRight="4dp"
                        android:weightSum="1">

                        <TextView
                            android:id="@+id/ch_free_service5"
                            android:layout_width="0dp"
                            android:layout_height="32dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size" />

                        <TextView
                            android:id="@+id/ch_free_service6"
                            android:layout_width="0dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            android:layout_margin="4dp"
                            android:layout_weight="0.50"
                            android:background="@drawable/freeservices_bg_img"
                            android:gravity="left|center_vertical"
                            android:paddingLeft="8dp"
                            android:text="text1"
                            android:textSize="@dimen/txtmemid_size"
                            android:visibility="invisible" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/activity_horizontal_margin"
                android:orientation="vertical"
                android:paddingLeft="4dp"
                android:paddingRight="4dp">

                <EditText
                    android:id="@+id/edt_subject_file"
                    android:layout_width="fill_parent"
                    android:layout_height="38dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/edt_bg"
                    android:hint="@string/subject_of_file"
                    android:padding="8dp"
                    android:textSize="@dimen/txtmemid_size" />

                <EditText
                    android:id="@+id/edt_instr_order"
                    android:layout_width="fill_parent"
                    android:layout_height="48dp"
                    android:background="@drawable/edt_bg"
                    android:gravity="top"
                    android:hint="@string/instructions_for_order"
                    android:padding="8dp"
                    android:textSize="@dimen/txtmemid_size" />
            </LinearLayout>


            <RelativeLayout
                android:id="@+id/lin_selected_files"
                android:layout_width="fill_parent"
                android:layout_height="158dp"

                android:layout_margin="8dp"
                android:layout_marginBottom="@dimen/lin_padding"
                android:background="@drawable/selected_files_lin_bg"

                android:orientation="vertical"
                android:padding="@dimen/btn_padding">

                <TextView
                    android:id="@+id/txtSelected"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/btn_padding"
                    android:text="@string/selected_files"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/txtid_size" />

                <ListView
                    android:id="@+id/lst_selected_files"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtSelected"
                    android:layout_gravity="center"
                    android:divider="@android:color/transparent"
                    android:gravity="center"
                    android:scrollbarThumbVertical="@color/colorPrimary"
                    android:scrollbars="vertical"
                    android:visibility="visible" />


            </RelativeLayout>

            <LinearLayout
                android:id="@+id/lin_btns"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/activity_horizontal_margin"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/activity_horizontal_margin"
                android:weightSum="1">

                <Button
                    android:id="@+id/btn_cancel"
                    android:layout_width="198dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/btn_padding"
                    android:layout_weight=".50"
                    android:background="@drawable/button_selector_start"
                    android:gravity="center"
                    android:text="@string/cancel"
                    android:textAllCaps="false"
                    android:textColor="@drawable/text_selector_start"
                    android:textSize="@dimen/txtid_size" />

                <Button
                    android:id="@+id/btn_order"
                    android:layout_width="198dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/btn_padding"
                    android:layout_weight=".50"
                    android:background="@drawable/button_selector_start"
                    android:gravity="center"
                    android:text="@string/order"
                    android:textAllCaps="false"
                    android:textColor="@drawable/text_selector_start"
                    android:textSize="@dimen/txtid_size" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>


</LinearLayout>